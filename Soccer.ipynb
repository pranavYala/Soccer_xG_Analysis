   "source": [
    "# Best outside the box shooters by ratio\n",
    "\n",
    "otb = attempts[attempts.location == 15.0]\n",
    "otb_players = otb.groupby('player').sum().reset_index()\n",
    "otb_players.rename(columns={'is_goal': 'Outside the box goals', 'prediction': 'expectedGoals_created'}, inplace=True)\n",
    "otb_players.expectedGoals_created = round(otb_players.expectedGoals_created, 2)\n",
    "otb_players['ratio'] = otb_players['Outside the box goals'] / otb_players['expectedGoals_created']\n",
    "\n",
    "disp = otb_players[otb_players['Outside the box goals'] > 5].sort_values('ratio', ascending=False).reset_index(drop=True)\n",
    "disp['rank'] = disp.index+1\n",
    "disp = disp[['rank', 'player', 'Outside the box goals', 'expectedGoals_created', 'ratio']].head(10)\n",
    "disp[['rank', 'player', 'Outside the box goals', 'expectedGoals_created', 'ratio']].head(5)"
   ]
